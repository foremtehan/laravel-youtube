{
  "name": "foremtehan/youtube",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0",
    "google/apiclient": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Youtube\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "scripts": {
    "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
  },
  "extra": {
    "google/apiclient-services": [
      "YouTube"
    ]
  }
}
